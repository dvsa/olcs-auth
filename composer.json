{
    "name": "olcs/olcs-auth",
    "description": "OLCS Authentication",
    "type": "library",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
            "type": "composer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.3.5",
        "doctrine/annotations": "^1.2.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5.3",
        "phpunit/phpunit": "^4.4.0",
        "mockery/mockery": "~0.9.3"
    },
    "autoload": {
        "psr-4": {
            "Dvsa\\Olcs\\Auth\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
